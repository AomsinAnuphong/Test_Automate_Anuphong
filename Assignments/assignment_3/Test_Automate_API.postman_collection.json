	"item": [
		{
			"name": "Get user profile success",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"",
							"pm.variables.set(\"expected_status\", \"200\");",
							"pm.variables.set(\"expected_id\", \"12\");",
							"pm.variables.set(\"expected_email\", \"rachel.howell@reqres.in\");",
							"pm.variables.set(\"expected_first_name\", \"Rachel\");",
							"pm.variables.set(\"expected_last_name\", \"Howell\");",
							"pm.variables.set(\"expected_avatar_url\", \"https://reqres.in/img/faces/12-image.jpg\")"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"",
							"pm.test(\"Verify response status code should be 200\", function () {",
							"    let expected_status = Number(pm.variables.get(\"expected_status\"));",
							"    pm.response.to.have.status(expected_status);",
							"});",
							"",
							"pm.test(\"Verify response body should contain user data\", function () {",
							"    let jsonData = pm.response.json();",
							"    let expected_id = Number(pm.variables.get(\"expected_id\"));",
							"    let expected_email = pm.variables.get(\"expected_email\");",
							"    let expected_first_name = pm.variables.get(\"expected_first_name\");",
							"    let expected_last_name = pm.variables.get(\"expected_last_name\");",
							"    let expected_avatar_url = pm.variables.get(\"expected_avatar_url\");",
							"    ",
							"    pm.expect(jsonData.data.id).to.equal(expected_id);",
							"    pm.expect(jsonData.data.email).to.equal(expected_email);",
							"    pm.expect(jsonData.data.first_name).to.equal(expected_first_name);",
							"    pm.expect(jsonData.data.last_name).to.equal(expected_last_name);",
							"    pm.expect(jsonData.data.avatar).to.equal(expected_avatar_url);",
							"",
							"});",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "x-api-key",
						"value": "reqres-free-v1",
						"type": "text"
					}
				],
				"url": {
					"raw": "https://reqres.in/api/users/12",
					"protocol": "https",
					"host": [
						"reqres",
						"in"
					],
					"path": [
						"api",
						"users",
						"12"
					]
				}
			},
			"response": []
		},
